{
    "name": "eskimo/base",
    "description": "The Eskimo CMS.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "git",
            "url": "http://gitlab.test/eskimo/core-module"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-debugbar": "3.2.1",
        "composer/installers": "~1.0",
        "esensi/model": "0.6.*",
        "eskimo/core-module": "1.0",
        "fideloper/proxy": "^4.0",
        "greabock/tentacles": "1.0",
        "igaster/laravel-theme": "2.*",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.4.0",
        "lavary/laravel-menu": "1.7.2",
        "nwidart/laravel-modules": "^4.0",
        "orchestra/asset": "~3.6",
        "paragonie/random_compat": "2.*",
        "spatie/laravel-schemaless-attributes": "1.3.0",
        "spatie/laravel-translation-loader": "2.2.*"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "secure-http": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}